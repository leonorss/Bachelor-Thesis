{
  "setReferenceGenomeAndMetaDataFrame":{
    "referenceGenome": "Data/TestReferenceData/ReferenceGenomeChromosome_22.fa",
    "metaDataFrame": "Data/TestReferenceData/MetaDataFrame.vcf"

  },

  "generateBinaryTree":{
    "numberOfLeaves": 4,
    "seed": 7
  },

  "createGermlineMutations":{
    "numberOfGermlineMutations": 100,
    "fractionOfHeterozygousGermlineMutations": 0.25,
    "seed": 56
  },

  "createMutations":{
    "numberOfMutations": 200,
    "seed": 490
  },

  "simulateReads":{
    "meanBinSize": 1000000,
    "standartDeviationOfBinSize": 1500,
    "seed": 7643,
    "meanMDACoverage": 3,
    "standartDeviationOfMDACoverage": 2,
    "zeroInflatationProbability": 0.5,
    "readLength": 100,
    "meanFragmentSize": 250,
    "standartDeviationOfFragmentSize": 16,
    "MDAamplificationErrorProbability": 0.0000001,
    "ARTcoverage": 1,
    "ARTDropReadProbability": 0.05
  }

}
